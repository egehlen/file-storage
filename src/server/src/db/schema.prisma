datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Account {
    id    Int     @id @default(autoincrement())
    name  String?
    email String  @unique
    passwordHash    String
}

model Session {
    id  Int @id @default(autoincrement())
    token String  @unique
    createdAt DateTime?
}